<odoo>
  <data>

     <record id="adjustment_report_form_view1" model="ir.ui.view">
            <field name="name">adjustment.report.form.view</field>
            <field name="model">inventory.adjustment.report</field>
            <field name="arch" type="xml">
                <form string="Adjustment Report">
                    <group>
                        <group>
                            <field name="start_date"/>

                        </group>
                        <group>
                            <field name="end_date"/>
                        </group>
                    </group>

                    <footer>
                        <button name="get_report_values" string="View Report" class="oe_highlight"
                                type="object" />

                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>


            </field>
        </record>



     <record id="open_top_sales_action1" model="ir.actions.act_window">
            <field name="name">Adjustment Report </field>
            <field name="res_model">inventory.adjustment.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    <menuitem id="adjustment_rep"
            action="open_top_sales_action1"
            name="Adjustment Report"
            parent="stock.menu_warehouse_report"
            sequence="4"/>
  </data>
</odoo>