<odoo>
  <data>

    <record model="ir.ui.view" id="view_partner_form122">
        <field name="name">res.partner.form.inherit122</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Import Template">
                   <group string="Upload File" name="mapping">
                       <field name="file" widget='file'></field>
                   </group>
                   <group string="Map Columns" name="mapping">
                       <field name="customer_sku" />
                       <field name="required_quantity"/>
                       <field name="quantity" />
                       <field name="uom" />
                       <field name="frequency_of_refill" />
                   </group>
                </page>
            </notebook>
        </field>
    </record>

  </data>
</odoo>
