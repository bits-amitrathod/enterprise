<odoo>

    <report id="action_todo_model_report"
            string="Purchase History"
            model="purchase.history.cust"
            report_type="qweb-html"
            file="purchase_history_custome.purchase_report"
            name="purchase_history_custome.purchase_report"
    />

    <template id="purchase_report">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <center>
                        <b>
                            <h4>Surgical Product Solutions</h4>
                        </b>
                        <b>
                            <h2>Purchase History Report Of Product</h2>
                        </b>
                    </center>
                    <center>
                        <b>
                            <span t-esc="start_date"/>
                            To
                            <span t-esc="end_date"/>
                        </b>
                    </center>
                    <!-- Report page content -->
                    <table class="table table-stripped">
                        <tr>
                            <th>Customer</th>
                            <th>Manufacturer</th>
                            <th>Product</th>
                            <th>Product Description</th>
                            <th>Qty in Stock</th>
                            <th>Price Per Stock</th>
                            <th>Min Exp Date</th>
                            <th>Max Exp Date</th>

                        </tr>

                        <t t-foreach="data.keys()" t-as="user">
                            <t t-foreach="data[user]" t-as="o">
                                <tr>
                                    <td class="col-xs-2">
                                        <span t-esc="o[0]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[5]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[1]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[9]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[4]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[6]"/>
                                    </td>
                                     <td class="col-xs-2">
                                        <span t-esc="o[10]"/>
                                    </td>
                                    <td class="col-xs-2">
                                        <span t-esc="o[11]"/>
                                    </td>
                                </tr>
                            </t>

                        </t>

                    </table>


                </div>
            </t>
        </t>
    </template>

</odoo>

