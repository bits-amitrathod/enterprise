<odoo>

    <report id="action_todo_model_report"
            string="Purchase History"
            model="purchase.history.cust"
            report_type="qweb-html"
            file="purchase_history_custome.purchase_report"
            name="purchase_history_custome.purchase_report"
    />

    <template id="purchase_report">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <center>
                           <b>
                            <h4>Surgical Product Solutions</h4>
                        </b>
                        <b>
                            <h2>Purchase History Report Of Product</h2>
                        </b>
                    </center>
                    <!-- Report page content -->
                    <table class="table table-stripped">
                        <tr>
                            <th>Product</th>
                            <th>Customer</th>
                            <th>Purchase Order</th>
                            <th>Shipped</th>
                            <th>Qty Shipped</th>
                            <th>price</th>

                        </tr>

                        <t t-foreach="data.keys()" t-as="user">
                            <t t-foreach="data[user]" t-as="o">
                                <tr>
                                    <td class="col-xs-2">
                                        <span t-esc="o[0]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[3]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[2]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[4]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[5]"/>
                                    </td>

                                    <td class="col-xs-2">
                                        <span t-esc="o[1]"/>
                                    </td>
                                </tr>
                            </t>

                        </t>

                    </table>


                </div>
            </t>
        </t>
    </template>

</odoo>
