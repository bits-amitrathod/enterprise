<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.actions.server" id="action_product_price_list_wizard">
            <field name="name"> product price list</field>
            <field name="model_id" ref="model_product_list_report"/>
            <field name="state">code</field>
            <field name="code">
                action = model.print_product_price_list_vise_report()
            </field>
         </record>

        <record model="ir.actions.server" id="action_customer_price_list_wizard">
            <field name="name"> product price list</field>
            <field name="model_id" ref="model_product_list_report"/>
            <field name="state">code</field>
            <field name="code">
                action = model.print_customer_product_price_list_vise_report()
            </field>
         </record>

        <menuitem id="menu_sale_report_product_list_wizard"
                  name="Product Price List"
                  action="action_product_price_list_wizard"
                  parent="stock.menu_warehouse_report"
                  sequence="1"/>

        <menuitem id="menu_sale_report_product_list_wizard1"
                  name="Customer Price List"
                  action="action_customer_price_list_wizard"
                  parent="stock.menu_warehouse_report"
                  sequence="1"/>
    </data>
</odoo>
