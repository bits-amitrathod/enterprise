<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.actions.server" id="action_server">
            <field name="name">saletest server</field>
            <field name="model_id" ref="model_custome_product_report"/>
            <field name="state">code</field>
            <field name="code">
                action = model.print_catalog_vise_report()
            </field>
         </record>

        <record model="ir.actions.server" id="action_product_catalog">
            <field name="name">product server</field>
            <field name="model_id" ref="model_custome_product_report"/>
            <field name="state">code</field>
            <field name="code">
                action = model.print_product_catalog_report()
            </field>
         </record>

    <menuitem id="sr_menu_custome_product_catalog_wizard"
                  name="Custom Product Catalog"
                  action="action_server"
                  parent="stock.menu_warehouse_report"
                  sequence="2"/>

    <menuitem id="sr_menu_product_catalog_wizard"
                  name="Product Catalog"
                  action="action_product_catalog"
                  parent="stock.menu_warehouse_report"
                  sequence="2"/>
    </data>
</odoo>
