<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="catalog_temp">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <page>
                    <div class="oe_structure"/>
                    <div class="row">
                    <center>
                        <b>
                            <h2> Surgical Product Solutions </h2>
                            <h4> Custom Product Catalog </h4>
                        </b>
                    </center>
                    </div>
                    <br/>
                    <t t-foreach="data.keys()" t-as="user">

                        <t t-set="total_sum" t-value="0"/>
                        <table class="table table-condensed" style="border: 1px solid black;">
                            <thead>
                                <tr style="border: 1px solid black;">
                                    <th style="border: 1px solid black;">Type</th>
                                    <th style="border: 1px solid black;">Manufacturer</th>
                                    <th style="border: 1px solid black;">Product Number</th>
                                    <th style="border: 1px solid black;">Product Name</th>
                                    <th style="border: 1px solid black;">Quantity</th>
                                    <th style="border: 1px solid black;">Price</th>
                                    <th style="border: 1px solid black;">Min Exp Date</th>
                                    <th style="border: 1px solid black;">Max Exp Date</th>

                                </tr>
                                <tr t-foreach="data[user]" t-as="order">
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[0]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[1]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[2]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[3]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[4]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        $ <span t-esc="order[5]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[6]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[7]"/>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                    </t>
                </page>
            </t>
        </t>
    </template>

    <template id="product_catalog_temp">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <page>
                    <div class="oe_structure"/>
                    <div class="row">
                    <center>
                        <b>
                            <h4> Product Catalog Report </h4>
                        </b>
                    </center>
                    </div>
                    <br/>
                    <t t-foreach="data.keys()" t-as="user">

                        <t t-set="total_sum" t-value="0"/>
                        <table class="table table-condensed" style="border: 1px solid black;">
                            <thead>
                                <tr style="border: 1px solid black;">
                                    <th style="border: 1px solid black;">Name</th>
                                    <th style="border: 1px solid black;">Price</th>
                                </tr>
                                <tr t-foreach="data[user]" t-as="order">
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[0]"/>
                                    </td>
                                    <td style="border: 1px solid black;">
                                        <span t-esc="order[1]"/>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                    </t>
                </page>
            </t>
        </t>
    </template>
</odoo>